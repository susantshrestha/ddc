<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title DDC Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <!-- DATA TABLES -->
    <link href="~/Content/dataTables.bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/AdminLTE.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/blue.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/skins/skin-blue.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body class="skin-blue sidebar-mini">
    <div class="wrapper">
        <!-- Main Header -->
        <header class="main-header">
            <!-- Logo -->
            <a href="index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>D</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Admin</b></span>
            </a>
            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    
@*                  
@*                    <form action="#" class="navbar-form navbar-left" role="search" method="get">*@
@*                        <div class="input-group">*@
@*                            <input type="text" class="form-control" id="navbar-search-input" placeholder="Search">*@
@*                            *@
@*                        </div>*@
@*                    </form>*@
                  


                    <ul class="nav navbar-nav">
    
                        <!-- User Account Menu -->
                        <li class="dropdown user user-menu">
                            <!-- Menu Toggle Button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <!-- The user image in the navbar-->
                                <img src="~/Content/img/avatar.png" class="user-image" alt="User Image"/>
                                <!-- hidden-xs hides the username on small devices so only the image appears. -->
                                <span class="hidden-xs">Alex Shrestha</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- The user image in the menu -->
                                <li class="user-header">
                                    <img src="~/Content/img/avatar.png" class="img-circle" alt="User Image"/>
                                    <p>
                                        Alex Shrestha - Web Developer
                                        <small>Member since Nov. 2012</small>
                                    </p>
                                </li>

                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat">Logout</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel (optional) -->
               
                <!-- search form (Optional) -->
               <!-- /.search form -->
                <!-- Sidebar Menu -->
                <ul class="sidebar-menu">
                    <!-- Optionally, you can add icons to the links -->
                    <li class="active"><a href="@Url.Action("Index", "Dash")"><i class="fa fa-list"></i> <span>Index</span></a></li>
                    <li><a href="@Url.Action("Index", "Bank")"><i class="fa fa-bank"></i> <span>Banks</span></a></li>
                    <li><a href="@Url.Action("Index", "Chise")"><i class="fa fa-link"></i> <span>Chise</span></a></li>
                    <li class="dropdown"><a href="@Url.Action("Index", "Shahakari")" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="fa fa-link"></i><span>Sahakari</span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>susant</li>
                            <li>alex</li>
                            <li>max</li>
                        </ul>
                    </li>
                    <li><a href="#"><i class="fa fa-download"></i> <span>Collection & Staff facility</span></a></li>
                    <li><a href="@Url.Action("Index", "Tankers")"><i class="fa fa-bus"></i> <span>Tankers</span></a></li>
                    <li><a href="#"><i class="fa fa-link" ></i> <span>Chalan</span></a></li>
                    <li><a href="#"><i class="fa fa-flask"></i> <span>Lab Receive</span></a></li>
                    <li><a href="#"><i class="fa fa-sticky-note-o"></i> <span>Credit Notes</span></a></li>
                    <li><a href="@Url.Action("Index", "Advance")"><i class="fa fa-link"></i> <span>Advance</span></a></li>
                    <li><a href="#"><i class="fa fa-money"></i> <span>Payments</span></a></li>
                    <li><a href="#"><i class="fa fa-info-circle"></i> <span>Reports</span></a></li>
                    <li><a href="#"><i class="fa fa-calculator"></i> <span>Incharge fine</span></a></li>
                    
                </ul><!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Page Header
                    <small>Optional description</small> 

                </h1>
@*                <ol class="breadcrumb">*@
@*                    <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>*@
@*                    <li class="active">Here</li>*@
                @*                </ol>*@
                
                @Html.MvcSiteMap().SiteMapPath()
            </section>
            <!-- Main content -->
            <div class="container-fluid">
               
                    <section class="content container-fluid">
                        <!-- Your Page Content Here -->

                        @RenderBody()


                    </section><!-- /.content -->
                
            </div>
        </div><!-- /.content-wrapper -->
    

        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
@*            <div class="pull-right hidden-xs">*@
@*                Anything you want*@
@*            </div>*@
            <!-- Default to the left -->
            <strong>Copyright &copy; 2015 <a href="#">Company</a>.</strong> All rights reserved.
        </footer>
       
        
</div><!-- ./wrapper -->

    <!-- REQUIRED JS SCRIPTS -->
    <!-- jQuery 2.1.4 -->
    <script src="~/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
    <!-- Bootstrap 3.3.2 JS -->
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <!-- AdminLTE App -->
    <script src="~/Scripts/app.min.js" type="text/javascript"></script>
    <!-- DATA TABES SCRIPT -->
    <script src="~/Scripts/plugins/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="~/Scripts/plugins/dataTables.bootstrap.min.js" type="text/javascript"></script>
    <script type="text/javascript">
      $(function () {
        $("#example1").DataTable();
        $('#example2').DataTable({
          "paging": true,
          "lengthChange": false,
          "searching": false,
          "ordering": true,
          "info": true,
          "autoWidth": false
        });
      });
    </script>
</body>
</html>